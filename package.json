{
  "name": "evi",
  "version": "0.9.7",
  "description": "evi application",
  "private": true,
  "scripts": {
    "version:sync": "node deployment/sync-version.js",
    "install:all": "cd back && npm install && cd ../front && npm install",
    "build:all": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd back && npm run build",
    "build:frontend": "cd front && npm run build",
    "test:all": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd back && npm test",
    "test:frontend": "cd front && npm run test-front:all",
    "dev:back": "cd back && npm run dev:back",
    "dev:front": "cd front && npm run dev:front",
    "start:backend": "cd back && npm start",
    "start:frontend": "cd front && npm run serve",
    "clean": "rm -rf back/dist front/dist back/node_modules front/node_modules",
    "podman:build": "podman compose -f deployment/podman-compose-dev.yml build",
    "podman:up": "podman compose -f deployment/podman-compose-dev.yml up -d",
    "podman:down": "podman compose -f deployment/podman-compose-dev.yml down",
    "podman:logs": "podman compose -f deployment/podman-compose-dev.yml logs -f",
    "build:local": "node deployment/build-dev.js",
    "build:local:clean": "node deployment/build-dev.js --clean"
  },
  "keywords": [
    "evi",
    "backend",
    "frontend",
    "database",
    "postgresql",
    "vue",
    "express",
    "typescript"
  ],
  "author": "evi team",
  "license": "ISC",
  "devDependencies": {
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
